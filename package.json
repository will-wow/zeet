{
  "name": "zeet",
  "version": "0.1.0",
  "author": "Will Ockelmann-Wagner",
  "license": "MIT",
  "private": true,
  "homepage": "https://myzeet.com",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.1",
    "@types/aframe": "^1.0.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "aframe-environment-component": "^2.0.0",
    "aframe-extras": "^6.1.0",
    "aframe-layout-component": "^5.3.0",
    "aframe-look-at-component": "^1.0.0",
    "face-api.js": "^0.22.2",
    "immer": "^6.0.9",
    "super-three": "^0.116.0",
    "typescript": "~3.9.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/webpack-env": "^1.15.3",
    "aframe-super-hot-html-loader": "^2.1.0",
    "aframe-super-hot-loader": "^1.7.0",
    "autoprefixer": "^7.2.3",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "3",
    "cross-env": "^7.0.2",
    "diff-dom": "^4.2.1",
    "gh-pages": "^3.1.0",
    "html-entities": "^1.2.1",
    "html-require-loader": "*",
    "ip": "^1.1.5",
    "jest": "^26.5.3",
    "npm-run-all": "^4.1.5",
    "nunjucks": "3.0.1",
    "prettier": "^2.0.5",
    "super-nunjucks-loader": "^2.0.0",
    "surge": "^0.21.6",
    "webpack": "^4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-sources": "1.0.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --progress --colors --hot-only --inline --port 3000",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "yarn jest",
    "test:ci": "yarn test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "npm-run-all lint:*",
    "lint:prettier": "yarn prettier --check './**/*.{ts,tsx,html,md,json}'",
    "format": "npm-run-all format:*",
    "format:prettier": "yarn prettier --write './**/*.{ts,tsx,html,md,json}'",
    "deploy": "gh-pages -m 'deploy [ci skip]' --dist ./build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ],
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
